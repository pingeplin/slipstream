# Google Drive Integration Test Configuration
# Copy this file to .env and update with your own test folder IDs

# Test folder with files (should contain files with MIME types: image/jpeg, image/png, or application/pdf)
TEST_GDRIVE_FOLDER_URL=https://drive.google.com/drive/folders/YOUR_FOLDER_ID_HERE
TEST_GDRIVE_FOLDER_ID=YOUR_FOLDER_ID_HERE

# Expected number of files in the test folder
TEST_GDRIVE_FILE_COUNT=3

# Empty test folder (should contain no files or only unsupported file types)
TEST_GDRIVE_EMPTY_FOLDER_ID=YOUR_EMPTY_FOLDER_ID_HERE
